<dl>
  <dt>Business Name</dt>
  <dd><%= @merchant.business_name %></dd>
  <dt>Contact name</dt>
  <dd><%= @merchant.contact_name %> <%= @merchant.contact_last_name %></dd>
  <dt>Business email address</dt>
  <dd><%= @merchant.email %></dd>
  <dt>Business type</dt>
  <dd><%= @merchant.business_type %></dd>
  <dt>Website</dt>
  <dd><%= @merchant.website %></dd>
  <dt>A little bit their business</dt>
  <dd><%= @merchant.comments %></dd>
  <dt>Phone</dt>
  <dd><%= @merchant.phone %></dd>
  <dt>Address</dt>
  <dd><%= @merchant.address1 %> <%= @merchant.address2 %>. <%= @merchant.city %> <%= @merchant.state %> <%= @merchant.zipcode %></dd>
</dl>
